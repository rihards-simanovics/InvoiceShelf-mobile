        "@types/react": "~18.2.45",
        "@types/react-test-renderer": "^18.0.7",
        "cz-conventional-changelog": "^3.3.0",
        "husky": "^8.0.0",
        "jest": "^29.2.1",
        "jest-expo": "~51.0.3",
        "react-test-renderer": "18.2.0",
        "standard-version": "^9.5.0",
        "typescript": "~5.3.3"
