    "jest": {
        "preset": "jest-expo"
    },
    "dependencies": {
        "@expo/vector-icons": "^14.0.2",
        "@react-navigation/native": "^6.0.2",
        "expo": "~51.0.23",
        "expo-constants": "~16.0.2",
        "expo-font": "~12.0.9",
        "expo-linking": "~6.3.1",
        "expo-router": "~3.5.19",
        "expo-splash-screen": "~0.27.5",
        "expo-status-bar": "~1.12.1",
        "expo-system-ui": "~3.0.7",
        "expo-web-browser": "~13.0.3",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-native": "0.74.3",
        "react-native-gesture-handler": "~2.16.1",
